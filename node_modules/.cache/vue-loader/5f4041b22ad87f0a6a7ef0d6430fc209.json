alStorage.getItem(\"pictures\")) || DATA,\n      value: \"\",\n      basket: [],\n      modal: null,\n    };\n  },\n  methods: {\n    getResult: function () {\n      // console.log(this.value);\n      if (this.value === \"\") {\n        console.log(\"nothing\");\n      }\n      if (this.value !== \"\") {\n        let res = DATA.filter( (i)=>{\n          return i.title\n            .toLowerCase()\n            .replace(/\\s+/g, \" \")\n            .trim()\n            .includes(this.value.replace(/\\s+/g, \" \").trim());\n        });\n        this.pictures = res;\n        console.log(this.pictures)\n        // localStorage.setItem('pictures', JSON.stringify(this.pictures));\n        // localStorage.setItem('data', JSON.stringify(this.pictures));\n      }\n    },\n    buyCard: function (e) {\n      setTimeout(()=> {\n        (this.pictures[e].state === \"В корзине\")\n          ? (this.pictures[e].state = \"Купить\")\n          : (this.pictures[e].state = \"В корзине\");\n        // DATA[e].state === \"В корзине\"\n        //   ? (DATA[e].state = \"Купить\")\n        //   : (DATA[e].state = \"В корзине\");\n        // console.log(DATA)\n        // this.pictures = DATA;\n        // console.log(this.pictures);\n         localStorage.setItem('pictures', JSON.stringify(this.pictures));\n        //  localStorage.setItem('data', JSON.stringify(DATA));\n      }, 1000);\n    },\n    show: function (e) {\n      this.modal = DATA[e];\n      console.log(this.modal);\n      console.log(e);\n    },\n    hide: function(){\n      this.modal = null;\n    }\n  },\n\n  name: \"App\",\n  components: {\n    Main,\n    Footer,\n    List,\n    Modal,\n  },\n};\n</script>\n\n<style>\n@import url(\"https://fonts.googleapis.com/css2?family=Merriweather:wght@300;400;700&display=swap\");\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"Merriweather\", serif;\n}\n#app {\n  height: 100vh;\n  display: flex;\n  justify-content: space-between;\n  flex-direction: column;\